+ ******************************************************************************/
+/* BD70528 adapter */
+typedef struct _srtm_bd70528_adapter
+{
+    struct _srtm_pmic_adapter adapter;
+    bd70528_handle_t *driver;
+} *srtm_bd70528_adapter_t;
